{
	"name": "klstudio",
	"version": "1.0.0",
	"description": "the website of K.L. Studio",
	"scripts": {
		"build": "webpack --config=./webpack.config.js --progress --hide-modules",
		"start": "node main.js",
		"dev": "cross-env NODE_ENV=development nodemon main.js",
		"dev:inspect": "cross-env NODE_ENV=development nodemon --inspect main.js",
		"node-inspect": "cross-env NODE_ENV=development nodemon --inspect",
		"lint": "./node_modules/.bin/eslint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/k-l-lambda/klstudio.git"
	},
	"nodemonConfig": {
		"ignore": [
			"app/*",
			"static/*",
			"html/*"
		],
		"delay": "80"
	},
	"author": "K.L.",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/k-l-lambda/klstudio/issues"
	},
	"devDependencies": {
		"@babel/core": "^7.2.0",
		"@tensorflow/tfjs": "^0.14.1",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.4",
		"cross-env": "^5.2.0",
		"css-loader": "^2.0.0",
		"dotenv": "^6.2.0",
		"dotenv-expand": "^4.2.0",
		"eslint": "^6.0.0",
		"eslint-plugin-html": "^5.0.5",
		"eslint-plugin-vue": "^5.2.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"ip": "^1.1.5",
		"nodemon": "^1.18.7",
		"postcss-loader": "^3.0.0",
		"style-loader": "^0.23.1",
		"url-loader": "^1.1.2",
		"vue": "^2.5.18",
		"vue-debounce": "^1.0.0",
		"vue-loader": "^15.4.2",
		"vue-resize-directive": "^1.1.4",
		"vue-template-compiler": "^2.5.18",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-middleware": "^3.4.0",
		"webpack-hot-middleware": "^2.24.3",
		"webpack-merge": "^4.1.4"
	},
	"dependencies": {
		"express": "^4.16.4"
	}
}