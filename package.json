  "three": "^0.126.1",
  "ts-node": "^8.4.1",
  "ts-node-dev": "^1.0.0",
  "url": "^0.11.4",
  "v-charts": "^1.19.0",
  "vue": "^3.4.21",
  "vue-class-component": "^7.0.2",
  "vue-debounce": "^1.0.0",
  "vue-property-decorator": "^8.3.0",
  "vue-resize-directive": "^1.2.0",
