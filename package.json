{
	"name": "klstudio",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"description": "the website of K.L. Studio",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"start": "node main.ts",
		"dev": "cross-env NODE_ENV=development ts-node-dev --project ./tsconfig.node.json main.ts",
		"dev:inspect": "cross-env NODE_ENV=development ts-node-dev --project ./tsconfig.node.json --inspect main.ts",
		"ts-dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev --project ./tsconfig.node.json --",
		"ts-dev:inspect": "ts-node-dev --project ./tsconfig.node.json --inspect --",
		"cube3-gentable6": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gentable.ts --outputFile=./static/data/cube3-table-6.json --depth=6",
		"cube3-gensolvermap": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gen-solver-map.ts",
		"cube3-genhash": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gen-hash.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/k-l-lambda/klstudio.git"
	},
	"nodemonConfig": {
		"ignore": [
			"app/*",
			"static/*",
			"html/*"
		],
		"delay": "80"
	},
	"author": "K.L.",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/k-l-lambda/klstudio/issues"
	},
	"devDependencies": {
		"@chrisoakman/chessboardjs": "^1.0.0",
		"@k-l-lambda/web-widgets": "^0.2.9",
		"@tensorflow/tfjs": "^1.2.6",
		"@types/events": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^2.24.0",
		"@typescript-eslint/parser": "^2.24.0",
		"@vue/cli-plugin-babel": "^4.1.0",
		"@vue/cli-plugin-eslint": "^4.1.0",
		"@vue/cli-plugin-router": "^4.1.0",
		"@vue/cli-plugin-typescript": "^4.1.0",
		"@vue/cli-service": "^4.1.0",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"chess.js": "^0.11.0",
		"core-js": "^3.4.4",
		"cross-env": "^5.2.0",
		"css-loader": "^2.0.0",
		"dotenv": "^6.2.0",
		"dotenv-expand": "^4.2.0",
		"echarts": "<5.0.0",
		"eslint": "^6.8.0",
		"eslint-plugin-vue": "^5.0.0",
		"events": "^3.2.0",
		"ip": "^1.1.5",
		"jquery": "^3.5.1",
		"mathjs": "^6.0.2",
		"node-sass": "^4.12.0",
		"nodemon": "^1.19.4",
		"pca-js": "^1.0.2",
		"sass-loader": "^8.0.0",
		"sha1": "^1.1.1",
		"three": "^0.105.2",
		"ts-node": "^8.4.1",
		"ts-node-dev": "^1.0.0",
		"v-charts": "^1.19.0",
		"vue": "^2.6.10",
		"vue-class-component": "^7.0.2",
		"vue-debounce": "^1.0.0",
		"vue-property-decorator": "^8.3.0",
		"vue-resize-directive": "^1.2.0",
		"vue-router": "^3.1.3",
		"vue-template-compiler": "^2.6.10"
	},
	"dependencies": {
		"@types/express": "^4.17.0",
		"express": "^4.16.4",
		"typescript": "~3.5.3",
		"yargs": "^13.2.4"
	}
}
