{
 "name": "klstudio",
 "version": "1.0.0",
 "private": true,
 "description": "the website of K.L. Studio",
 "scripts": {
  "serve": "cross-env VUE_CLI_IPC=none vue-cli-service serve",
  "build": "cross-env VUE_CLI_IPC=none VUE_CLI_MODERN_MODE=0 vue-cli-service build",
  "lint": "vue-cli-service lint",
  "start": "cross-env TS_NODE_PROJECT=./tsconfig.node.json node -r ts-node/register main.ts",
  "dev": "cross-env NODE_ENV=development ts-node-dev --project ./tsconfig.node.json main.ts",
  "dev:inspect": "cross-env NODE_ENV=development ts-node-dev --project ./tsconfig.node.json --inspect main.ts",
  "ts-dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev --project ./tsconfig.node.json --",
  "ts-dev:inspect": "ts-node-dev --project ./tsconfig.node.json --inspect --",
  "cube3-gentable6": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gentable.ts --outputFile=./static/data/cube3-table-6.json --depth=6",
  "cube3-gensolvermap": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gen-solver-map.ts",
  "cube3-genhash": "cross-env NODE_OPTIONS='--max-old-space-size=4096' ts-node-dev -- ./tools/cube3-gen-hash.ts"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/k-l-lambda/klstudio.git"
 },
 "nodemonConfig": {
  "ignore": [
   "app/*",
   "static/*",
   "html/*"
  ],
  "delay": "80"
 },
 "author": "K.L.",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/k-l-lambda/klstudio/issues"
 },
 "engines": {
  "node": ">=21.7.1"
 },
 "devDependencies": {
  "@chrisoakman/chessboardjs": "^1.0.0",
  "@k-l-lambda/lotus": "1.0.2",
  "@k-l-lambda/music-widgets": "1.0.1",
  "@tensorflow/tfjs": "^1.2.6",
  "@types/events": "^3.0.0",
  "@typescript-eslint/eslint-plugin": "^5.62.0",
  "@typescript-eslint/parser": "^5.62.0",
  "@vue/cli-plugin-babel": "^5.0.9",
  "@vue/cli-plugin-eslint": "^5.0.9",
  "@vue/cli-plugin-router": "^5.0.9",
  "@vue/cli-plugin-typescript": "^5.0.9",
  "@vue/cli-service": "^5.0.9",
  "@vue/compat": "^3.4.21",
  "@vue/compiler-sfc": "^3.4.21",
  "@vue/eslint-config-standard": "^4.0.0",
  "@vue/eslint-config-typescript": "^4.0.0",
  "chess.js": "^0.11.0",
  "child-process-promise": "^2.2.1",
  "color": "^5.0.2",
  "core-js": "^3.4.4",
  "crc-32": "^1.2.2",
  "cross-env": "^5.2.0",
  "css-loader": "^6.8.1",
  "diff": "^8.0.2",
  "dotenv": "^6.2.0",
  "dotenv-expand": "^4.2.0",
  "echarts": "<5.0.0",
  "eslint": "^8.57.0",
  "eslint-plugin-vue": "^9.18.1",
  "events": "^3.2.0",
  "formidable": "^3.5.4",
  "ip": "^1.1.5",
